= turbo_stream.replace @marking_note do
  = render partial: 'marking_notes/marking_note', locals: {marking_note: @marking_note, objs: [@course, @group, @submission, @marked_point, @marking_note]}

= turbo_stream.replace dom_id(@marked_point, :xvalue_of) do
  %span{id: dom_id(@marked_point, :xvalue_of)}= prog2_point_achieved @marked_point

= turbo_stream.replace dom_id(@submission, :marked_for) do
  %span{id: dom_id(@submission, :marked_for)}= @submission.marked_for
